{
  "name": "todos-restful-server",
  "version": "1.0.0",
  "description": "restful server for react-todo-flavors",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "PORT=3002 DEBUG=server-rest:* nodemon ./bin/www",
    "model-tester": "mocha \"models/*.test.js\"",
    "route-tester": "mocha \"routes/*.test.js\"",
    "test": "yarn model-tester && concurrently --success first -k \"NODE_ENV=test PORT=3004 node ./bin/www\" \"yarn route-tester\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "supertest": "^3.4.2"
  }
}
